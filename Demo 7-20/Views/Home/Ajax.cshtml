@{
    ViewBag.Title = "Ajax";
}
<hr />
<div class="content-div">
    <div class="title"></div>
    <br />
    <div class="venue-title"></div>
    <br />
    <div class="row">
        <div class="col-md-6">
            <h4 class="date"></h4>
        </div>
        <div class="col-md-6">
            <h4 class="address"></h4>
        </div>
    </div>
    <input type="text" id="start-input" class="controls" placeholder="Enter Your Location" />
    <div id="map"> </div>
    <div class="images-div"></div>
</div>
@section scripts
{
    <script src="~/Scripts/Ajax.js"></script>
    <script src="~/Scripts/jquery-dateFormat.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBbnDo9mZQiQ4vAUVJRSG5r4r3ONlqsZHY&callback=initMap&libraries=places" async defer></script>
    @*<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBbnDo9mZQiQ4vAUVJRSG5r4r3ONlqsZHY"></script>*@

